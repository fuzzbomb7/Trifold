@model Trifold.Models.EventPromoters
@{
	ViewData["Title"] = "Edit Promoter";
}

@section Styles {
	<link rel="stylesheet" href="~/vendor/libs/select2/select2.css">
}

<div class="card">
	<h3 class="card-header">@ViewData["Title"]</h3>
	<div class="card-body">
		@await Html.PartialAsync("_Promoters", Model)
	</div>
</div>

@section Scripts {
	<script src="~/vendor/libs/select2/select2.js"></script>
	<script>
		$(document).ready(function () {
			$('#select-app').select2({ placeholder: "Select an App" });
		});
	</script>
}

